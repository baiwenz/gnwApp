@Route(path = "/my/login/LoginFragment")
public class LoginFragment extends BaseFragment<ILoginView, LoginPressenter<ILoginView>> implements ILoginView,View.OnClickListener{
    private static LoginFragment loginFragment;
    private Button mGotoReg;

    public static LoginFragment newInstance(String param1) {
        loginFragment = new LoginFragment();
        Bundle args = new Bundle();
        args.putString("agrs1", param1);
        loginFragment.setArguments(args);
        return loginFragment;
    }
    public LoginFragment(){

    }
    @Override
    public void onClick(View v) {
        if (v.getId() == R.id.gotoregister) {
            ARouter.getInstance().build( Constance.LOGIN_ACTIVITY)
                    .withInt("view", 1)
                    .navigation(getActivity(), 1002);
        }
    }

    @Override
    protected LoginPressenter<ILoginView> createPresenter() {
        return null;
    }

    @Override
    protected int initLayoutInflater() {
        return R.layout.layout_login;
    }

    @Override
    public void initView(View view) {
        mGotoReg=view.findViewById(R.id.gotoregister);
    }

    @Override
    public void initData() {
        mGotoReg.setOnClickListener(this);
    }

    @Override
    public void Code(Message message) {

    }

    @Override
    public void showLoading() {

    }

    @Override
    public void hideLoading(boolean b) {

    }

    @Override
    public void onError(Object result) {

    }
